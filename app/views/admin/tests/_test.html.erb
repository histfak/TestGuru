<tr>
  <td><%= test.title %></td>
  <td class="text-center"><%= test.level %></td>
  <td class="text-center"><%= test.questions.count %></td>
  <td class="text-center"><%= link_to t('links.show'), admin_test_path(test) %></td>
  <td class="text-center"><%= link_to t('links.edit'), edit_admin_test_path(test) %></td>
  <td class="text-center">
    <%= link_to t('links.delete'),
                admin_test_path(test),
                method: :delete,
                data: { confirm: t('links.confirm') } %>
  </td>
</tr>
