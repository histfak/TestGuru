<h1><%= question_header(@question) %></h1>

<%= form_with model: [:admin, @test, @question], local: true do |form| %>

  <%= render 'shared/errors', recource: @question %>

  <p>
    <%= form.label :body %><br>
    <%= form.text_field :body %>
  </p>

  <%= form.submit %>
<% end %>
<p><%= link_to 'Back', tests_path %></p>
